{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: g++ build active file",
            "command": "/usr/bin/g++",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${workspaceFolder}/mains/main.cpp",
                "$(workspaceFolder)/srcs/gameengine/GameEngine.cpp",
                "$(workspaceFolder)/srcs/players/BotPlayer.cpp",
                "$(workspaceFolder)/srcs/agents/MinMaxAgent.cpp",
                "$(workspaceFolder)/srcs/agents/MinMaxAlphaBetaAgent.cpp",
                "$(workspaceFolder)/srcs/agents/MctsAgent.cpp",
                "$(workspaceFolder)/srcs/heuristics/BestHeuristic.cpp",
                "$(workspaceFolder)/srcs/heuristics/RolloutHeuristic.cpp",
                "-o",
                "${workspaceFolder}/bins/debug"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}